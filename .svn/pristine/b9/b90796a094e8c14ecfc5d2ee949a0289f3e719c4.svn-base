<Window x:Class="RealEstateApp.MapWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{Binding Source={StaticResource CultureResources},Path=[YerevanMap]}" WindowState="Maximized" WindowStyle="ToolWindow" ShowInTaskbar="True" KeyUp="Window_KeyUp"
        Icon="pack://application:,,,/Shared;component/Images/Application.png">
    <Border Margin="3" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" CornerRadius="4" >
        <WindowsFormsHost HorizontalAlignment="Stretch" VerticalAlignment="Stretch" x:Name="mapPlace"/>
    </Border>
</Window>
