<Application x:Class="DatabaseManagement.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:Cultures="clr-namespace:RealEstate.Common.Cultures;assembly=RealEstate.Common" StartupUri="MainWindow.xaml" Startup="Application_Startup" DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\SilverlightStyle.xaml"/>
                <ResourceDictionary Source="Styles\Generic.xaml"/>
            </ResourceDictionary.MergedDictionaries>

			<ObjectDataProvider x:Key="CultureResources" ObjectType="{x:Type Cultures:CultureResources}"/>
		</ResourceDictionary>
    </Application.Resources>
</Application>
